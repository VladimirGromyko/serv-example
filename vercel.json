{
  "version": 2,
  "name": "serv-example",
  "builds": [
    { "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/favicon.ico",
      "headers": { "Cache-Control": "public, max-age=3600" },
      "continue": true
    }
  ]
}
